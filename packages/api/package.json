{
  "name": "@heedback/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "imports": {
    "#start/*": "./build/start/*.js",
    "#config/*": "./build/config/*.js",
    "#controllers/*": "./build/app/controllers/*.js",
    "#models/*": "./build/app/models/*.js",
    "#services/*": "./build/app/services/*.js",
    "#exceptions/*": "./build/app/exceptions/*.js",
    "#validators/*": "./build/app/validators/*.js",
    "#middleware/*": "./build/app/middleware/*.js",
    "#helpers/*": "./build/app/helpers/*.js"
  },
  "scripts": {
    "dev": "node --import=tsx/esm bin/server.ts --watch",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node bin/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "node --import=@swc-node/register/esm-register bin/console.ts test",
    "migration:run": "node --import=@swc-node/register/esm-register bin/console.ts migration:run",
    "migration:rollback": "node --import=@swc-node/register/esm-register bin/console.ts migration:rollback",
    "migration:fresh": "node --import=@swc-node/register/esm-register bin/console.ts migration:fresh",
    "seed": "node --import=@swc-node/register/esm-register bin/console.ts db:seed"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.0",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/drive": "^3.4.1",
    "@adonisjs/lucid": "^21.3.0",
    "@adonisjs/redis": "^9.1.0",
    "@adonisjs/session": "^7.5.0",
    "@adonisjs/static": "^1.1.1",
    "@aws-sdk/client-s3": "^3.992.0",
    "@aws-sdk/s3-request-presigner": "^3.992.0",
    "@vinejs/vine": "^2.1.0",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc-node/register": "^1.10.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
